<div class="row">
    <div class="col m12">
        <h4>Registro de personal</h4>
        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
            <%= devise_error_messages! %>

            <div class="input-field col m12">
                <i class="material-icons prefix">mail_outline</i>
                <%= f.email_field :email, autofocus: true, placeholder:"Email :",class:"col m10 offset-m1" %>
            </div>

            <div class="input-field col m12">
                <i class="material-icons prefix">fingerprint</i>
                <%= f.password_field :password, autocomplete: "off", placeholder:"Contraseña :",class:"col m10 offset-m1" %>
            </div>

            <div class="input-field col m12">
                <i class="material-icons prefix">fingerprint</i>
                <%= f.password_field :password_confirmation, autocomplete: "off", placeholder:"Confirmación :",class:"col m10 offset-m1" %>
            </div>

            <div class="actions">
                <%= f.submit "Registrar", class:"btn col offset-m9" %>
            </div>
        <% end %>
        <%= render "devise/shared/links" %>
    </div>
</div>